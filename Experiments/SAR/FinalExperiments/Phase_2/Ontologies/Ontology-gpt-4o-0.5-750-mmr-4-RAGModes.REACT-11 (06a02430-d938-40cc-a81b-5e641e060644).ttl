@prefix : <http://example.org/SAR#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Incident a owl:Class ;
    rdfs:label "Incident" ;
    rdfs:comment "Class representing an incident during a wildfire." .

:WeatherCondition a owl:Class ;
    rdfs:label "WeatherCondition" ;
    rdfs:comment "Class representing weather conditions affecting wildfires." .

:Observation a owl:Class ;
    rdfs:label "Observation" ;
    rdfs:comment "Class representing observations from various sources." .

:Mission a owl:Class ;
    rdfs:label "Mission" ;
    rdfs:comment "Class representing a mission during a wildfire incident." .

:Service a owl:Class ;
    rdfs:label "Service" ;
    rdfs:comment "Class representing services provided during a wildfire incident." .

:hasLocation a owl:ObjectProperty ;
    rdfs:domain :Incident, :Observation, :Mission ;
    rdfs:range :Location ;
    rdfs:label "hasLocation" ;
    rdfs:comment "Property linking an entity to its location." .

:hasPriority a owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string ;
    rdfs:label "hasPriority" ;
    rdfs:comment "Property representing the priority of an incident." .

:hasUrgency a owl:DatatypeProperty ;
    rdfs:domain :Incident, :Mission ;
    rdfs:range xsd:string ;
    rdfs:label "hasUrgency" ;
    rdfs:comment "Property representing the urgency of an incident or mission." .

:hasTemperature a owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float ;
    rdfs:label "hasTemperature" ;
    rdfs:comment "Property representing the temperature in weather conditions." .

:hasWindSpeed a owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float ;
    rdfs:label "hasWindSpeed" ;
    rdfs:comment "Property representing the wind speed in weather conditions." .

:hasHumidity a owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float ;
    rdfs:label "hasHumidity" ;
    rdfs:comment "Property representing the humidity in weather conditions." .

:hasForecast a owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:string ;
    rdfs:label "hasForecast" ;
    rdfs:comment "Property representing the weather forecast." .

:hasSource a owl:DatatypeProperty ;
    rdfs:domain :Observation ;
    rdfs:range xsd:string ;
    rdfs:label "hasSource" ;
    rdfs:comment "Property representing the source of an observation." .

:hasCreationDate a owl:DatatypeProperty ;
    rdfs:domain :Observation ;
    rdfs:range xsd:dateTime ;
    rdfs:label "hasCreationDate" ;
    rdfs:comment "Property representing the creation date of an observation." .

:hasClassificationType a owl:DatatypeProperty ;
    rdfs:domain :Observation ;
    rdfs:range xsd:string ;
    rdfs:label "hasClassificationType" ;
    rdfs:comment "Property representing the classification type of smoke." .

:hasSupportService a owl:ObjectProperty ;
    rdfs:domain :Mission ;
    rdfs:range :Service ;
    rdfs:label "hasSupportService" ;
    rdfs:comment "Property linking a mission to its support service." .

:hasPopulationDensity a owl:DatatypeProperty ;
    rdfs:domain :Location ;
    rdfs:range xsd:float ;
    rdfs:label "hasPopulationDensity" ;
    rdfs:comment "Property representing the population density in a location." .

:hasInvolvedPeople a owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range :Person ;
    rdfs:label "hasInvolvedPeople" ;
    rdfs:comment "Property linking an incident to the involved people." .

:Location a owl:Class ;
    rdfs:label "Location" ;
    rdfs:comment "Class representing a geographical location." .

:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "Class representing a person involved in an incident." .

:VulnerableObject a owl:Class ;
    rdfs:label "VulnerableObject" ;
    rdfs:comment "Class representing an object vulnerable to wildfire." .

:hasVulnerableObject a owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range :VulnerableObject ;
    rdfs:label "hasVulnerableObject" ;
    rdfs:comment "Property linking an incident to vulnerable objects." .

:Firebreak a :VulnerableObject ;
    rdfs:label "Firebreak" ;
    rdfs:comment "Class representing a firebreak." .

:ForestRoad a :VulnerableObject ;
    rdfs:label "ForestRoad" ;
    rdfs:comment "Class representing a forest road." .

:hasStatus a owl:DatatypeProperty ;
    rdfs:domain :VulnerableObject ;
    rdfs:range xsd:string ;
    rdfs:label "hasStatus" ;
    rdfs:comment "Property representing the status of a vulnerable object." .