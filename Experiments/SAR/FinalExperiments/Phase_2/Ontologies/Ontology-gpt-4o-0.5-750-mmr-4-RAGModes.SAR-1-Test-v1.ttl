@prefix : <http://example.org/sar#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Incident rdf:type owl:Class .
:Impact rdf:type owl:Class .
:WeatherCondition rdf:type owl:Class .
:SensorData rdf:type owl:Class .
:SatelliteData rdf:type owl:Class .
:SocialMediaData rdf:type owl:Class .
:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .
:FirefightingService rdf:type owl:Class .
:SupportService rdf:type owl:Class .
:AreaBurnt rdf:type owl:Class .
:Building rdf:type owl:Class .
:Vehicle rdf:type owl:Class .
:Machinery rdf:type owl:Class .
:Rescue rdf:type owl:Class .
:Animal rdf:type owl:Class .
:Bird rdf:type owl:Class .
:Hive rdf:type owl:Class .
:PossibleCauseOfFire rdf:type owl:Class .
:ForcesInvolved rdf:type owl:Class .

:incidentType rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:priority rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:urgency rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:location rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:temperature rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float .

:windSpeed rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float .

:humidity rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float .

:forecast rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:string .

:dataType rdf:type owl:DatatypeProperty ;
    rdfs:domain :SensorData ;
    rdfs:range xsd:string .

:creationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :SensorData ;
    rdfs:range xsd:dateTime .

:missionType rdf:type owl:DatatypeProperty ;
    rdfs:domain :Mission ;
    rdfs:range xsd:string .

:servicesInvolved rdf:type owl:ObjectProperty ;
    rdfs:domain :Mission ;
    rdfs:range :Service .

:forestArea rdf:type owl:DatatypeProperty ;
    rdfs:domain :AreaBurnt ;
    rdfs:range xsd:float .

:swampArea rdf:type owl:DatatypeProperty ;
    rdfs:domain :AreaBurnt ;
    rdfs:range xsd:float .

:woodlandArea rdf:type owl:DatatypeProperty ;
    rdfs:domain :AreaBurnt ;
    rdfs:range xsd:float .

:kind rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building, :Vehicle, :Machinery ;
    rdfs:range xsd:string .

:insurance rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building, :Vehicle, :Machinery ;
    rdfs:range xsd:string .

:operatorName rdf:type owl:DatatypeProperty ;
    rdfs:domain :Building, :Vehicle, :Machinery ;
    rdfs:range xsd:string .

:cause rdf:type owl:DatatypeProperty ;
    rdfs:domain :Rescue ;
    rdfs:range xsd:string .

:fireOfficer rdf:type owl:DatatypeProperty ;
    rdfs:domain :ForcesInvolved ;
    rdfs:range xsd:string .

:volunteerFirefighter rdf:type owl:DatatypeProperty ;
    rdfs:domain :ForcesInvolved ;
    rdfs:range xsd:string .

:armedForcesPersonnel rdf:type owl:DatatypeProperty ;
    rdfs:domain :ForcesInvolved ;
    rdfs:range xsd:string .

:municipalityStaff rdf:type owl:DatatypeProperty ;
    rdfs:domain :ForcesInvolved ;
    rdfs:range xsd:string .

:publicSectorStaff rdf:type owl:DatatypeProperty ;
    rdfs:domain :ForcesInvolved ;
    rdfs:range xsd:string .

:clubMember rdf:type owl:DatatypeProperty ;
    rdfs:domain :ForcesInvolved ;
    rdfs:range xsd:string .

:individual rdf:type owl:DatatypeProperty ;
    rdfs:domain :ForcesInvolved ;
    rdfs:range xsd:string .