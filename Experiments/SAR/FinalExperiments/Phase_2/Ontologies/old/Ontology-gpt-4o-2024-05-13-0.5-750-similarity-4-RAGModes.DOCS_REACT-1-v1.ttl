@prefix : <http://www.example.org/wildfire-ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Incident rdf:type owl:Class .
:FireIncident rdf:type owl:Class ;
    rdfs:subClassOf :Incident .

:Impact rdf:type owl:Class .
:WeatherCondition rdf:type owl:Class .
:SensorData rdf:type owl:Class .
:SocialMediaData rdf:type owl:Class ;
    rdfs:subClassOf :SensorData .

:Mission rdf:type owl:Class .
:FirefightingMission rdf:type owl:Class ;
    rdfs:subClassOf :Mission .

:Service rdf:type owl:Class .

:location rdf:type owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:severity rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:priority rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:status rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:temperature rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float .

:windSpeed rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float .

:humidity rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float .

:forecast rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:string .

:source rdf:type owl:DatatypeProperty ;
    rdfs:domain :SensorData ;
    rdfs:range xsd:string .

:creationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :SensorData ;
    rdfs:range xsd:dateTime .

:itemLocation rdf:type owl:DatatypeProperty ;
    rdfs:domain :SensorData ;
    rdfs:range xsd:string .

:supportsMission rdf:type owl:ObjectProperty ;
    rdfs:domain :Service ;
    rdfs:range :Mission .

:vulnerableObject rdf:type owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:forestryWorkStatus rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:populationDensity rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:integer .

:peopleLocation rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

:smokeClassification rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string .

