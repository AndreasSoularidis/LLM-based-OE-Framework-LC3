@prefix : <http://example.com/sar#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Incident rdf:type owl:Class .
:Impact rdf:type owl:Class .
:Location rdf:type owl:Class .
:WeatherCondition rdf:type owl:Class .
:Temperature rdf:type owl:Class .
:WindSpeed rdf:type owl:Class .
:Humidity rdf:type owl:Class .
:WeatherForecast rdf:type owl:Class .
:SensorData rdf:type owl:Class .
:SatelliteData rdf:type owl:Class .
:SocialMediaData rdf:type owl:Class .
:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .
:FirstResponder rdf:type owl:Class .
:VulnerableObject rdf:type owl:Class .
:Firebreak rdf:type owl:Class .
:AccessToForestRoad rdf:type owl:Class .

:hasLocation rdf:type owl:ObjectProperty .
:hasImpact rdf:type owl:ObjectProperty .
:hasWeatherCondition rdf:type owl:ObjectProperty .
:hasTemperature rdf:type owl:ObjectProperty .
:hasWindSpeed rdf:type owl:ObjectProperty .
:hasHumidity rdf:type owl:ObjectProperty .
:hasForecast rdf:type owl:ObjectProperty .
:hasCurrentMeasurement rdf:type owl:ObjectProperty .
:hasCreationDate rdf:type owl:DatatypeProperty .
:hasPriority rdf:type owl:DatatypeProperty .
:isMostUrgent rdf:type owl:DatatypeProperty .
:hasClassificationType rdf:type owl:DatatypeProperty .
:hasStatus rdf:type owl:DatatypeProperty .
:hasMission rdf:type owl:ObjectProperty .
:hasService rdf:type owl:ObjectProperty .
:hasPopulationDensity rdf:type owl:DatatypeProperty .
:hasInvolvedPeople rdf:type owl:ObjectProperty .

:Incident rdfs:comment "Represents an incident during a wildfire."^^xsd:string .
:Impact rdfs:comment "Represents the impact of a wildfire incident."^^xsd:string .
:Location rdfs:comment "Represents the location of an incident or mission."^^xsd:string .
:WeatherCondition rdfs:comment "Represents weather conditions affecting the wildfire."^^xsd:string .
:Temperature rdfs:comment "Represents the temperature during a wildfire."^^xsd:string .
:WindSpeed rdfs:comment "Represents the wind speed during a wildfire."^^xsd:string .
:Humidity rdfs:comment "Represents the humidity during a wildfire."^^xsd:string .
:WeatherForecast rdfs:comment "Represents the weather forecast for a location."^^xsd:string .
:SensorData rdfs:comment "Represents data collected from sensors."^^xsd:string .
:SatelliteData rdfs:comment "Represents data collected from satellites."^^xsd:string .
:SocialMediaData rdfs:comment "Represents data collected from social media."^^xsd:string .
:Mission rdfs:comment "Represents a mission in wildfire management."^^xsd:string .
:Service rdfs:comment "Represents a service involved in wildfire management."^^xsd:string .
:FirstResponder rdfs:comment "Represents a first responder in a wildfire incident."^^xsd:string .
:VulnerableObject rdfs:comment "Represents a vulnerable object involved in a wildfire incident."^^xsd:string .
:Firebreak rdfs:comment "Represents the status of firebreaks in wildfire forestry works."^^xsd:string .
:AccessToForestRoad rdfs:comment "Represents the status of access to forest roads in wildfire forestry works."^^xsd:string .

:Incident owl:equivalentClass [
  rdf:type owl:Class ;
  owl:unionOf (:Impact :Location :WeatherCondition)
] .

:WeatherCondition owl:equivalentClass [
  rdf:type owl:Class ;
  owl:unionOf (:Temperature :WindSpeed :Humidity :WeatherForecast)
] .

:Mission owl:equivalentClass [
  rdf:type owl:Class ;
  owl:intersectionOf (:Service :Location)
] .

:VulnerableObject owl:equivalentClass [
  rdf:type owl:Class ;
  owl:unionOf (:Firebreak :AccessToForestRoad)
] .

:hasLocation rdfs:domain :Incident .
:hasLocation rdfs:range :Location .

:hasImpact rdfs:domain :Incident .
:hasImpact rdfs:range :Impact .

:hasWeatherCondition rdfs:domain :Incident .
:hasWeatherCondition rdfs:range :WeatherCondition .

:hasTemperature rdfs:domain :WeatherCondition .
:hasTemperature rdfs:range :Temperature .

:hasWindSpeed rdfs:domain :WeatherCondition .
:hasWindSpeed rdfs:range :WindSpeed .

:hasHumidity rdfs:domain :WeatherCondition .
:hasHumidity rdfs:range :Humidity .

:hasForecast rdfs:domain :WeatherCondition .
:hasForecast rdfs:range :WeatherForecast .

:hasCurrentMeasurement rdfs:domain :WeatherCondition .
:hasCurrentMeasurement rdfs:range :WeatherCondition .

:hasCreationDate rdfs:domain :SensorData .
:hasCreationDate rdfs:range xsd:dateTime .

:hasPriority rdfs:domain :Mission .
:hasPriority rdfs:range xsd:string .

:isMostUrgent rdfs:domain :Mission .
:isMostUrgent rdfs:range xsd:boolean .

:hasClassificationType rdfs:domain :Incident .
:hasClassificationType rdfs:range xsd:string .

:hasStatus rdfs:domain :VulnerableObject .
:hasStatus rdfs:range xsd:string .

:hasMission rdfs:domain :Service .
:hasMission rdfs:range :Mission .

:hasService rdfs:domain :Mission .
:hasService rdfs:range :Service .

:hasPopulationDensity rdfs:domain :Location .
:hasPopulationDensity rdfs:range xsd:integer .

:hasInvolvedPeople rdfs:domain :Incident .
:hasInvolvedPeople rdfs:range :FirstResponder .