@prefix : <http://example.com/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:WildfireIncident rdf:type owl:Class .
:Impact rdf:type owl:Class .
:BurntArea rdf:type owl:Class .
:AffectedBuildings rdf:type owl:Class .
:AffectedVehicles rdf:type owl:Class .
:RescuedAnimals rdf:type owl:Class .
:RescuedPeople rdf:type owl:Class .
:Temperature rdf:type owl:Class .
:WindSpeed rdf:type owl:Class .
:Humidity rdf:type owl:Class .
:WeatherForecast rdf:type owl:Class .
:SensorData rdf:type owl:Class .
:SatelliteData rdf:type owl:Class .
:SocialMediaData rdf:type owl:Class .
:FirefightingMission rdf:type owl:Class .
:SupportService rdf:type owl:Class .
:EmergencyResponse rdf:type owl:Class .
:ServiceLocation rdf:type owl:Class .
:vulnerableObject rdf:type owl:Class .
:urgentIncident rdf:type owl:Class .
:urgentIncident rdfs:subClassOf :WildfireIncident .
:urgentMission rdf:type owl:Class .
:urgentMission rdfs:subClassOf :FirefightingMission .

:areaSize rdf:type owl:DatatypeProperty .
:areaSize rdfs:domain :BurntArea .
:areaSize rdfs:range xsd:float .

:areaType rdf:type owl:DatatypeProperty .
:areaType rdfs:domain :BurntArea .
:areaType rdfs:range xsd:string .

:location rdf:type owl:DatatypeProperty .
:location rdfs:domain :BurntArea .
:location rdfs:range xsd:string .

:hasTemperature rdf:type owl:ObjectProperty .
:hasTemperature rdfs:domain :WeatherForecast .
:hasTemperature rdfs:range :Temperature .

:hasWindSpeed rdf:type owl:ObjectProperty .
:hasWindSpeed rdfs:domain :WeatherForecast .
:hasWindSpeed rdfs:range :WindSpeed .

:hasHumidity rdf:type owl:ObjectProperty .
:hasHumidity rdfs:domain :WeatherForecast .
:hasHumidity rdfs:range :Humidity .

:hasSensorData rdf:type owl:ObjectProperty .
:hasSensorData rdfs:domain :WildfireIncident .
:hasSensorData rdfs:range :SensorData .

:hasSatelliteData rdf:type owl:ObjectProperty .
:hasSatelliteData rdfs:domain :WildfireIncident .
:hasSatelliteData rdfs:range :SatelliteData .

:hasSocialMediaData rdf:type owl:ObjectProperty .
:hasSocialMediaData rdfs:domain :WildfireIncident .
:hasSocialMediaData rdfs:range :SocialMediaData .

:hasFirefightingMission rdf:type owl:ObjectProperty .
:hasFirefightingMission rdfs:domain :SupportService .
:hasFirefightingMission rdfs:range :FirefightingMission .

:hasServiceLocation rdf:type owl:ObjectProperty .
:hasServiceLocation rdfs:domain :SupportService .
:hasServiceLocation rdfs:range :ServiceLocation .

:hasEmergencyResponse rdf:type owl:ObjectProperty .
:hasEmergencyResponse rdfs:domain :WildfireIncident .
:hasEmergencyResponse rdfs:range :EmergencyResponse .

:incidentLocation rdf:type owl:DatatypeProperty .
:incidentLocation rdfs:domain :WildfireIncident .
:incidentLocation rdfs:range xsd:string .

:incidentPriority rdf:type owl:DatatypeProperty .
:incidentPriority rdfs:domain :WildfireIncident .
:incidentPriority rdfs:range xsd:string .

:currentTemperature rdf:type owl:DatatypeProperty .
:currentTemperature rdfs:domain :Temperature .
:currentTemperature rdfs:range xsd:float .

:currentWindSpeed rdf:type owl:DatatypeProperty .
:currentWindSpeed rdfs:domain :WindSpeed .
:currentWindSpeed rdfs:range xsd:float .

:currentHumidity rdf:type owl:DatatypeProperty .
:currentHumidity rdfs:domain :Humidity .
:currentHumidity rdfs:range xsd:float .

:forecastTemperature rdf:type owl:DatatypeProperty .
:forecastTemperature rdfs:domain :WeatherForecast .
:forecastTemperature rdfs:range xsd:float .

:forecastWindSpeed rdf:type owl:DatatypeProperty .
:forecastWindSpeed rdfs:domain :WeatherForecast .
:forecastWindSpeed rdfs:range xsd:float .

:forecastHumidity rdf:type owl:DatatypeProperty .
:forecastHumidity rdfs:domain :WeatherForecast .
:forecastHumidity rdfs:range xsd:float .

:creationDate rdf:type owl:DatatypeProperty .
:creationDate rdfs:domain :SensorData, :SatelliteData, :SocialMediaData .
:creationDate rdfs:range xsd:dateTime .

:dataLocation rdf:type owl:DatatypeProperty .
:dataLocation rdfs:domain :SensorData, :SatelliteData, :SocialMediaData .
:dataLocation rdfs:range xsd:string .

:smokeClassification rdf:type owl:DatatypeProperty .
:smokeClassification rdfs:domain :SensorData .
:smokeClassification rdfs:range xsd:string .

:involvedVulnerableObject rdf:type owl:ObjectProperty .
:involvedVulnerableObject rdfs:domain :WildfireIncident .
:involvedVulnerableObject rdfs:range :vulnerableObject .

:forestryWorkStatus rdf:type owl:DatatypeProperty .
:forestryWorkStatus rdfs:domain :WildfireIncident .
:forestryWorkStatus rdfs:range xsd:string .

:offeredService rdf:type owl:DatatypeProperty .
:offeredService rdfs:domain :SupportService .
:offeredService rdfs:range xsd:string .

:followedMission rdf:type owl:ObjectProperty .
:followedMission rdfs:domain :SupportService .
:followedMission rdfs:range :FirefightingMission .

:missionLocation rdf:type owl:DatatypeProperty .
:missionLocation rdfs:domain :FirefightingMission .
:missionLocation rdfs:range xsd:string .

:populationDensity rdf:type owl:DatatypeProperty .
:populationDensity rdfs:domain :ServiceLocation .
:populationDensity rdfs:range xsd:float .

:peopleLocation rdf:type owl:DatatypeProperty .
:peopleLocation rdfs:domain :ServiceLocation .
:peopleLocation rdfs:range xsd:string .

:BurntArea rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :areaSize ;
  owl:someValuesFrom xsd:float
] .

:BurntArea rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :areaType ;
  owl:someValuesFrom xsd:string
] .

:BurntArea rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :location ;
  owl:someValuesFrom xsd:string
] .

:Temperature rdf:type owl:Class .
:Temperature rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasTemperature ;
  owl:someValuesFrom xsd:float
] .

:WindSpeed rdf:type owl:Class .
:WindSpeed rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasWindSpeed ;
  owl:someValuesFrom xsd:float
] .

:Humidity rdf:type owl:Class .
:Humidity rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasHumidity ;
  owl:someValuesFrom xsd:float
] .