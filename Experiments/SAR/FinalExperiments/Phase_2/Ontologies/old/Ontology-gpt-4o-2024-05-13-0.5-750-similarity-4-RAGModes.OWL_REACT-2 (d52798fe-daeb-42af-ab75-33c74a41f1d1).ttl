@prefix : <http://example.com/SARWildfire#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/SARWildfire> rdf:type owl:Ontology .

### Classes
:Incident rdf:type owl:Class .
:Fire rdf:type owl:Class ; rdfs:subClassOf :Incident .
:Evacuation rdf:type owl:Class ; rdfs:subClassOf :Incident .
:Injury rdf:type owl:Class ; rdfs:subClassOf :Incident .

:WeatherCondition rdf:type owl:Class .
:Temperature rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:WindSpeed rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:Humidity rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:WeatherForecast rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .

:ObservationData rdf:type owl:Class .
:SensorData rdf:type owl:Class ; rdfs:subClassOf :ObservationData .
:SatelliteData rdf:type owl:Class ; rdfs:subClassOf :ObservationData .
:SocialMediaData rdf:type owl:Class ; rdfs:subClassOf :ObservationData .

:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .

### Properties
:hasWeatherCondition rdf:type owl:ObjectProperty ;
                     rdfs:domain :Incident ;
                     rdfs:range :WeatherCondition .

:hasObservationData rdf:type owl:ObjectProperty ;
                    rdfs:domain :Incident ;
                    rdfs:range :ObservationData .

:involvesService rdf:type owl:ObjectProperty ;
                 rdfs:domain :Mission ;
                 rdfs:range :Service .

:hasLocation rdf:type owl:DatatypeProperty ;
             rdfs:domain :Incident ;
             rdfs:range xsd:string .

:hasPriority rdf:type owl:DatatypeProperty ;
             rdfs:domain :Incident ;
             rdfs:range xsd:string .

:isCriticalForFire rdf:type owl:DatatypeProperty ;
                  rdfs:domain :WeatherCondition ;
                  rdfs:range xsd:boolean .

:currentMeasurement rdf:type owl:DatatypeProperty ;
                    rdfs:domain :WeatherCondition ;
                    rdfs:range xsd:float .

:forecastMeasurement rdf:type owl:DatatypeProperty ;
                     rdfs:domain :WeatherCondition ;
                     rdfs:range xsd:float .

:hasUrgency rdf:type owl:DatatypeProperty ;
            rdfs:domain :Incident ;
            rdfs:range xsd:string .

:hasCreationDate rdf:type owl:DatatypeProperty ;
                 rdfs:domain :ObservationData ;
                 rdfs:range xsd:dateTime .

:hasItemLocation rdf:type owl:DatatypeProperty ;
                 rdfs:domain :ObservationData ;
                 rdfs:range xsd:string .

:hasSmokeClassification rdf:type owl:DatatypeProperty ;
                        rdfs:domain :ObservationData ;
                        rdfs:range xsd:string .

:involvesVulnerableObject rdf:type owl:ObjectProperty ;
                          rdfs:domain :Incident ;
                          rdfs:range :ObservationData .

:hasForestryWorkStatus rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Incident ;
                       rdfs:range xsd:string .

:providesSupport rdf:type owl:ObjectProperty ;
                 rdfs:domain :Service ;
                 rdfs:range :Mission .

:hasMissionLocation rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Mission ;
                    rdfs:range xsd:string .

:hasPopulationDensity rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Mission ;
                      rdfs:range xsd:float .

:hasInvolvedPeopleLocation rdf:type owl:DatatypeProperty ;
                           rdfs:domain :Mission ;
                           rdfs:range xsd:string .