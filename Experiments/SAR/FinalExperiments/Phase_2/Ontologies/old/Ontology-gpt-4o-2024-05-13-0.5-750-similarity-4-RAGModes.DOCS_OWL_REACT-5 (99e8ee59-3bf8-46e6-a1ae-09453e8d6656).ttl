@prefix : <http://example.com/sar#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Incident rdf:type owl:Class .
:WildfireIncident rdf:type owl:Class ; rdfs:subClassOf :Incident .
:Impact rdf:type owl:Class .
:Priority rdf:type owl:Class .
:WeatherCondition rdf:type owl:Class .
:Temperature rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:WindSpeed rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:Humidity rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:Forecast rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:SensorData rdf:type owl:Class .
:SocialMediaData rdf:type owl:Class ; rdfs:subClassOf :SensorData .
:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .
:Location rdf:type owl:Class .
:Firefighting rdf:type owl:Class ; rdfs:subClassOf :Service .
:FirstResponder rdf:type owl:Class ; rdfs:subClassOf :Service .

:hasImpact rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Impact .
:hasPriority rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Priority .
:hasLocation rdf:type owl:ObjectProperty ; rdfs:domain :Incident, :Mission ; rdfs:range :Location .
:hasTemperature rdf:type owl:DatatypeProperty ; rdfs:domain :WeatherCondition ; rdfs:range xsd:float .
:hasWindSpeed rdf:type owl:DatatypeProperty ; rdfs:domain :WeatherCondition ; rdfs:range xsd:float .
:hasHumidity rdf:type owl:DatatypeProperty ; rdfs:domain :WeatherCondition ; rdfs:range xsd:float .
:hasForecast rdf:type owl:ObjectProperty ; rdfs:domain :WeatherCondition ; rdfs:range :Forecast .
:hasDataSource rdf:type owl:ObjectProperty ; rdfs:domain :SensorData ; rdfs:range :Service .
:hasCreationDate rdf:type owl:DatatypeProperty ; rdfs:domain :SensorData ; rdfs:range xsd:dateTime .
:providesSupport rdf:type owl:ObjectProperty ; rdfs:domain :Service ; rdfs:range :Mission .
:isUrgent rdf:type owl:DatatypeProperty ; rdfs:domain :Mission ; rdfs:range xsd:boolean .
:hasStatus rdf:type owl:DatatypeProperty ; rdfs:domain :Mission ; rdfs:range xsd:string .
:isPartOf rdf:type owl:ObjectProperty ; rdfs:domain :Mission ; rdfs:range :Service .

:Incident rdfs:comment "Represents any incident that occurs during a wildfire."^^xsd:string .
:WildfireIncident rdfs:comment "Represents an incident specifically related to wildfires."^^xsd:string .
:Impact rdfs:comment "Represents the impact of an incident."^^xsd:string .
:Priority rdfs:comment "Represents the priority level of an incident."^^xsd:string .
:WeatherCondition rdfs:comment "Represents weather conditions relevant to wildfire incidents."^^xsd:string .
:Temperature rdfs:comment "Represents the temperature in a weather condition."^^xsd:string .
:WindSpeed rdfs:comment "Represents the wind speed in a weather condition."^^xsd:string .
:Humidity rdfs:comment "Represents the humidity in a weather condition."^^xsd:string .
:Forecast rdfs:comment "Represents the weather forecast."^^xsd:string .
:SensorData rdfs:comment "Represents data collected from sensors."^^xsd:string .
:SocialMediaData rdfs:comment "Represents data collected from social media."^^xsd:string .
:Mission rdfs:comment "Represents a mission undertaken during a wildfire incident."^^xsd:string .
:Service rdfs:comment "Represents a service involved in wildfire management."^^xsd:string .
:Firefighting rdfs:comment "Represents firefighting services."^^xsd:string .
:FirstResponder rdfs:comment "Represents first responder services."^^xsd:string .
:Location rdfs:comment "Represents the location of an incident or mission."^^xsd:string .
:providesSupport rdfs:comment "Indicates the support provided by a service for a mission."^^xsd:string .
:isUrgent rdfs:comment "Indicates whether a mission is urgent."^^xsd:string .
:hasStatus rdfs:comment "Indicates the status of a mission."^^xsd:string .
:isPartOf rdfs:comment "Indicates the service a mission is part of."^^xsd:string .