:WildfireIncident rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasImpact ;
  owl:someValuesFrom :Impact
] .

:WildfireIncident rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasLocation ;
  owl:someValuesFrom :IncidentLocation
] .

