@prefix : <http://example.com/sar-wildfire#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/sar-wildfire> rdf:type owl:Ontology .

### Classes
:Wildfire rdf:type owl:Class ;
    rdfs:label "Wildfire" ;
    rdfs:comment "Represents a wildfire incident." .

:WeatherCondition rdf:type owl:Class ;
    rdfs:label "WeatherCondition" ;
    rdfs:comment "Represents weather conditions relevant to wildfires." .

:SensorData rdf:type owl:Class ;
    rdfs:label "SensorData" ;
    rdfs:comment "Represents data collected from sensors." .

:SatelliteData rdf:type owl:Class ;
    rdfs:label "SatelliteData" ;
    rdfs:comment "Represents data collected from satellites." .

:SocialMediaData rdf:type owl:Class ;
    rdfs:label "SocialMediaData" ;
    rdfs:comment "Represents data collected from social media." .

:Mission rdf:type owl:Class ;
    rdfs:label "Mission" ;
    rdfs:comment "Represents a mission in response to a wildfire." .

:Service rdf:type owl:Class ;
    rdfs:label "Service" ;
    rdfs:comment "Represents a service involved in wildfire management." .

:Location rdf:type owl:Class ;
    rdfs:label "Location" ;
    rdfs:comment "Represents a geographical location." .

### Properties
:hasLocation rdf:type owl:ObjectProperty ;
    rdfs:domain :Wildfire ;
    rdfs:range :Location ;
    rdfs:label "hasLocation" ;
    rdfs:comment "Indicates the location of a wildfire." .

:hasSeverity rdf:type owl:DatatypeProperty ;
    rdfs:domain :Wildfire ;
    rdfs:range xsd:string ;
    rdfs:label "hasSeverity" ;
    rdfs:comment "Indicates the severity of a wildfire." .

:hasPriority rdf:type owl:DatatypeProperty ;
    rdfs:domain :Wildfire ;
    rdfs:range xsd:string ;
    rdfs:label "hasPriority" ;
    rdfs:comment "Indicates the priority of a wildfire incident." .

:hasWeatherCondition rdf:type owl:ObjectProperty ;
    rdfs:domain :Wildfire ;
    rdfs:range :WeatherCondition ;
    rdfs:label "hasWeatherCondition" ;
    rdfs:comment "Indicates the weather conditions relevant to a wildfire." .

:hasSensorData rdf:type owl:ObjectProperty ;
    rdfs:domain :Wildfire ;
    rdfs:range :SensorData ;
    rdfs:label "hasSensorData" ;
    rdfs:comment "Indicates sensor data relevant to a wildfire." .

:hasSatelliteData rdf:type owl:ObjectProperty ;
    rdfs:domain :Wildfire ;
    rdfs:range :SatelliteData ;
    rdfs:label "hasSatelliteData" ;
    rdfs:comment "Indicates satellite data relevant to a wildfire." .

:hasSocialMediaData rdf:type owl:ObjectProperty ;
    rdfs:domain :Wildfire ;
    rdfs:range :SocialMediaData ;
    rdfs:label "hasSocialMediaData" ;
    rdfs:comment "Indicates social media data relevant to a wildfire." .

:hasMission rdf:type owl:ObjectProperty ;
    rdfs:domain :Service ;
    rdfs:range :Mission ;
    rdfs:label "hasMission" ;
    rdfs:comment "Indicates a mission undertaken by a service." .

:hasService rdf:type owl:ObjectProperty ;
    rdfs:domain :Mission ;
    rdfs:range :Service ;
    rdfs:label "hasService" ;
    rdfs:comment "Indicates the service involved in a mission." .

:hasTemperature rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float ;
    rdfs:label "hasTemperature" ;
    rdfs:comment "Indicates the temperature during a wildfire." .

:hasWindSpeed rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float ;
    rdfs:label "hasWindSpeed" ;
    rdfs:comment "Indicates the wind speed during a wildfire." .

:hasHumidity rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float ;
    rdfs:label "hasHumidity" ;
    rdfs:comment "Indicates the humidity during a wildfire." .

:hasForecast rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:string ;
    rdfs:label "hasForecast" ;
    rdfs:comment "Indicates the weather forecast relevant to a wildfire." .

:hasCreationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :SensorData, :SatelliteData, :SocialMediaData ;
    rdfs:range xsd:dateTime ;
    rdfs:label "hasCreationDate" ;
    rdfs:comment "Indicates the creation date of the data." .

:hasClassificationType rdf:type owl:DatatypeProperty ;
    rdfs:domain :SensorData ;
    rdfs:range xsd:string ;
    rdfs:label "hasClassificationType" ;
    rdfs:comment "Indicates the classification type of smoke." .

:involvesVulnerableObjects rdf:type owl:ObjectProperty ;
    rdfs:domain :Wildfire ;
    rdfs:range :Location ;
    rdfs:label "involvesVulnerableObjects" ;
    rdfs:comment "Indicates the vulnerable objects involved in a wildfire incident." .

:hasStatus rdf:type owl:DatatypeProperty ;
    rdfs:domain :Location ;
    rdfs:range xsd:string ;
    rdfs:label "hasStatus" ;
    rdfs:comment "Indicates the status of wildfire forestry works." .

:offersSupport rdf:type owl:ObjectProperty ;
    rdfs:domain :Service ;
    rdfs:range :Mission ;
    rdfs:label "offersSupport" ;
    rdfs:comment "Indicates the support offered by a service." .

:hasPopulationDensity rdf:type owl:DatatypeProperty ;
    rdfs:domain :Location ;
    rdfs:range xsd:float ;
    rdfs:label "hasPopulationDensity" ;
    rdfs:comment "Indicates the population density in an area." .

:hasInvolvedPeople rdf:type owl:ObjectProperty ;
    rdfs:domain :Wildfire ;
    rdfs:range :Location ;
    rdfs:label "hasInvolvedPeople" ;
    rdfs:comment "Indicates the location of people involved in the wildfire." .