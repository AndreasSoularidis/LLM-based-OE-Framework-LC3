@prefix : <http://example.com/sar#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:WildfireIncident rdf:type owl:Class .
:WeatherCondition rdf:type owl:Class .
:SensorData rdf:type owl:Class .
:SocialMediaData rdf:type owl:Class .
:FirstResponderInput rdf:type owl:Class .
:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .

:Temperature rdf:type owl:Class .
:WindSpeed rdf:type owl:Class .
:Humidity rdf:type owl:Class .

:WeatherCondition owl:unionOf ( :Temperature :WindSpeed :Humidity ) .

:hasImpact rdf:type owl:ObjectProperty .
:hasPriority rdf:type owl:DatatypeProperty .

:hasCreationDate rdf:type owl:DatatypeProperty .
:hasLocation rdf:type owl:ObjectProperty .
:hasClassificationType rdf:type owl:DatatypeProperty .

:offersSupport rdf:type owl:ObjectProperty .
:followsMission rdf:type owl:ObjectProperty .
:involvesPeople rdf:type owl:ObjectProperty .

:hasMeasurement rdf:type owl:DatatypeProperty .
:hasForecast rdf:type owl:DatatypeProperty .

:hasMissionLocation rdf:type owl:ObjectProperty .
:hasUrgentMissionLocation rdf:type owl:ObjectProperty .
:hasPopulationDensity rdf:type owl:DatatypeProperty .

:Temperature rdfs:comment "Represents the temperature in the wildfire area."^^xsd:string .
:WindSpeed rdfs:comment "Represents the wind speed in the wildfire area."^^xsd:string .
:Humidity rdfs:comment "Represents the humidity level in the wildfire area."^^xsd:string .