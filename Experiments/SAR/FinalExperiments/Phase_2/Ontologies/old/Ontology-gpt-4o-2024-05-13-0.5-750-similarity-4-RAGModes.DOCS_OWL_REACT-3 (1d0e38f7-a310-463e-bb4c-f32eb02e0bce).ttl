@prefix : <http://example.com/sar#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Incident rdf:type owl:Class .
:WildfireIncident rdf:type owl:Class ; rdfs:subClassOf :Incident .
:Impact rdf:type owl:Class ; rdfs:subClassOf :Incident .
:Priority rdf:type owl:Class ; rdfs:subClassOf :Incident .
:hasLocation rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Location .
:hasPriority rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Priority .
:hasImpact rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Impact .
:FormOfFire rdf:type owl:Class .
:HazardIndex rdf:type owl:Class .
:NotificationMethod rdf:type owl:Class .
:FireServiceResponsibility rdf:type owl:Class .

:WeatherCondition rdf:type owl:Class .
:temperature rdf:type owl:DatatypeProperty ; rdfs:domain :WeatherCondition ; rdfs:range xsd:float .
:windSpeed rdf:type owl:DatatypeProperty ; rdfs:domain :WeatherCondition ; rdfs:range xsd:float .
:humidity rdf:type owl:DatatypeProperty ; rdfs:domain :WeatherCondition ; rdfs:range xsd:float .
:weatherForecast rdf:type owl:DatatypeProperty ; rdfs:domain :WeatherCondition ; rdfs:range xsd:string .

:Observation rdf:type owl:Class .
:hasSource rdf:type owl:ObjectProperty ; rdfs:domain :Observation ; rdfs:range xsd:string .
:creationDate rdf:type owl:DatatypeProperty ; rdfs:domain :Observation ; rdfs:range xsd:dateTime .
:location rdf:type owl:ObjectProperty ; rdfs:domain :Observation ; rdfs:range :Location .
:classificationType rdf:type owl:DatatypeProperty ; rdfs:domain :Observation ; rdfs:range xsd:string .

:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .
:supportsService rdf:type owl:ObjectProperty ; rdfs:domain :Mission ; rdfs:range :Service .
:followsMission rdf:type owl:ObjectProperty ; rdfs:domain :Service ; rdfs:range :Mission .
:missionLocation rdf:type owl:ObjectProperty ; rdfs:domain :Mission ; rdfs:range :Location .

:Location rdf:type owl:Class .
:PopulationDensity rdf:type owl:Class .
:hasPopulationDensity rdf:type owl:ObjectProperty ; rdfs:domain :Location ; rdfs:range :PopulationDensity .
:involvedPeople rdf:type owl:ObjectProperty ; rdfs:domain :Location ; rdfs:range :Person .

:Person rdf:type owl:Class .

:Incident owl:equivalentClass [
  rdf:type owl:Restriction ;
  owl:onProperty :hasLocation ;
  owl:someValuesFrom :Location
] .

:UrgentIncident rdf:type owl:Class ; rdfs:subClassOf :Incident ;
  owl:equivalentClass [
    rdf:type owl:Restriction ;
    owl:onProperty :hasPriority ;
    owl:someValuesFrom [
      rdf:type owl:Class ;
      owl:oneOf ( :HighPriority )
    ]
  ] .

:HighPriority rdf:type :Priority .

:WeatherCondition owl:equivalentClass [
  rdf:type owl:Class ;
  owl:intersectionOf (
    [ rdf:type owl:Restriction ;
      owl:onProperty :temperature ;
      owl:someValuesFrom xsd:float
    ]
    [ rdf:type owl:Restriction ;
      owl:onProperty :windSpeed ;
      owl:someValuesFrom xsd:float
    ]
    [ rdf:type owl:Restriction ;
      owl:onProperty :humidity ;
      owl:someValuesFrom xsd:float
    ]
  )
] .

:Mission owl:equivalentClass [
  rdf:type owl:Class ;
  owl:intersectionOf (
    [ rdf:type owl:Restriction ;
      owl:onProperty :missionLocation ;
      owl:someValuesFrom :Location
    ]
    [ rdf:type owl:Restriction ;
      owl:onProperty :supportsService ;
      owl:someValuesFrom :Service
    ]
  )
] .

:Service owl:equivalentClass [
  rdf:type owl:Class ;
  owl:intersectionOf (
    [ rdf:type owl:Restriction ;
      owl:onProperty :followsMission ;
      owl:someValuesFrom :Mission
    ]
  )
] .