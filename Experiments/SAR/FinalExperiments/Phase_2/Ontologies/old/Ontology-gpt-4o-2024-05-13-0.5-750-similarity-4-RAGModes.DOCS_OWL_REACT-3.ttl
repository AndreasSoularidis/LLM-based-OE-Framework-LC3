@prefix : <http://example.com/sar#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Incident rdf:type owl:Class .
:Wildfire rdf:type owl:Class ;
          rdfs:subClassOf :Incident .

:WeatherCondition rdf:type owl:Class .
:Temperature rdf:type owl:Class ;
             rdfs:subClassOf :WeatherCondition .
:WindSpeed rdf:type owl:Class ;
           rdfs:subClassOf :WeatherCondition .
:Humidity rdf:type owl:Class ;
          rdfs:subClassOf :WeatherCondition .

:HumanEarthObservation rdf:type owl:Class .
:SensorData rdf:type owl:Class ;
            rdfs:subClassOf :HumanEarthObservation .
:SatelliteData rdf:type owl:Class ;
               rdfs:subClassOf :HumanEarthObservation .
:SocialMediaData rdf:type owl:Class ;
                 rdfs:subClassOf :HumanEarthObservation .

:Mission rdf:type owl:Class .
:FirefightingMission rdf:type owl:Class ;
                     rdfs:subClassOf :Mission .
:RescueMission rdf:type owl:Class ;
               rdfs:subClassOf :Mission .
:EvacuationMission rdf:type owl:Class ;
                   rdfs:subClassOf :Mission .

:Service rdf:type owl:Class .

:Location rdf:type owl:Class .

:Person rdf:type owl:Class .

:Animal rdf:type owl:Class .

:Building rdf:type owl:Class .

:Vehicle rdf:type owl:Class .

:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:domain :Incident, :Mission, :HumanEarthObservation ;
             rdfs:range :Location .

:hasPriority rdf:type owl:DatatypeProperty ;
             rdfs:domain :Incident ;
             rdfs:range xsd:string .

:hasUrgency rdf:type owl:DatatypeProperty ;
            rdfs:domain :Incident, :Mission ;
            rdfs:range xsd:string .

:hasTemperature rdf:type owl:DatatypeProperty ;
                rdfs:domain :WeatherCondition ;
                rdfs:range xsd:float .

:hasWindSpeed rdf:type owl:DatatypeProperty ;
              rdfs:domain :WeatherCondition ;
              rdfs:range xsd:float .

:hasHumidity rdf:type owl:DatatypeProperty ;
             rdfs:domain :WeatherCondition ;
             rdfs:range xsd:float .

:hasForecast rdf:type owl:DatatypeProperty ;
             rdfs:domain :WeatherCondition ;
             rdfs:range xsd:string .

:hasCreationDate rdf:type owl:DatatypeProperty ;
                 rdfs:domain :HumanEarthObservation ;
                 rdfs:range xsd:dateTime .

:hasDataType rdf:type owl:DatatypeProperty ;
             rdfs:domain :HumanEarthObservation ;
             rdfs:range xsd:string .

:hasService rdf:type owl:ObjectProperty ;
            rdfs:domain :Mission ;
            rdfs:range :Service .

:hasIncident rdf:type owl:ObjectProperty ;
             rdfs:domain :Mission ;
             rdfs:range :Incident .

:hasObservation rdf:type owl:ObjectProperty ;
                rdfs:domain :Incident ;
                rdfs:range :HumanEarthObservation .

:hasMission rdf:type owl:ObjectProperty ;
            rdfs:domain :Service ;
            rdfs:range :Mission .

:hasPopulationDensity rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Location ;
                      rdfs:range xsd:float .

:hasInvolvedPeople rdf:type owl:ObjectProperty ;
                   rdfs:domain :Incident ;
                   rdfs:range :Person .

:hasInvolvedAnimals rdf:type owl:ObjectProperty ;
                    rdfs:domain :Incident ;
                    rdfs:range :Animal .

:hasInvolvedBuildings rdf:type owl:ObjectProperty ;
                      rdfs:domain :Incident ;
                      rdfs:range :Building .

:hasInvolvedVehicles rdf:type owl:ObjectProperty ;
                     rdfs:domain :Incident ;
                     rdfs:range :Vehicle .

:hasBurntArea rdf:type owl:DatatypeProperty ;
              rdfs:domain :Incident ;
              rdfs:range xsd:float .

:hasSmokeClassification rdf:type owl:DatatypeProperty ;
                        rdfs:domain :HumanEarthObservation ;
                        rdfs:range xsd:string .

:FirefightingService rdf:type owl:Class ;
                     rdfs:subClassOf :Service .

:RescueService rdf:type owl:Class ;
               rdfs:subClassOf :Service .

:EvacuationService rdf:type owl:Class ;
                   rdfs:subClassOf :Service .

:hasFirebreakStatus rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Location ;
                   rdfs:range xsd:string .

:hasAccessStatus rdf:type owl:DatatypeProperty ;
                rdfs:domain :Location ;
                rdfs:range xsd:string .