@prefix : <http://example.com/owl/wildfire/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/owl/wildfire> rdf:type owl:Ontology .

:WildfireIncident rdf:type owl:Class .
:WeatherCondition rdf:type owl:Class .
:Observation rdf:type owl:Class .
:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .
:Location rdf:type owl:Class .
:Person rdf:type owl:Class .
:Firebreak rdf:type owl:Class .
:AccessToForestRoads rdf:type owl:Class .
:AreaType rdf:type owl:Class .

:Temperature rdf:type owl:Class .
:Temperature rdfs:subClassOf :WeatherCondition .

:WindSpeed rdf:type owl:Class .
:WindSpeed rdfs:subClassOf :WeatherCondition .

:Humidity rdf:type owl:Class .
:Humidity rdfs:subClassOf :WeatherCondition .

:Forecast rdf:type owl:Class .
:Forecast rdfs:subClassOf :WeatherCondition .

:SensorData rdf:type owl:Class .
:SensorData rdfs:subClassOf :Observation .

:SatelliteData rdf:type owl:Class .
:SatelliteData rdfs:subClassOf :Observation .

:SocialMediaData rdf:type owl:Class .
:SocialMediaData rdfs:subClassOf :Observation .

:hasCreationDate rdf:type owl:DatatypeProperty .
:hasCreationDate rdfs:domain :Observation .
:hasCreationDate rdfs:range xsd:dateTime .

:hasLocation rdf:type owl:ObjectProperty .
:hasLocation rdfs:domain :Observation, :WildfireIncident, :Mission .
:hasLocation rdfs:range :Location .

:hasClassification rdf:type owl:DatatypeProperty .
:hasClassification rdfs:domain :Observation .
:hasClassification rdfs:range xsd:string .

:hasDate rdf:type owl:DatatypeProperty .
:hasDate rdfs:domain :WildfireIncident .
:hasDate rdfs:range xsd:dateTime .

:hasPriority rdf:type owl:DatatypeProperty .
:hasPriority rdfs:domain :WildfireIncident .
:hasPriority rdfs:range xsd:integer .

:hasCause rdf:type owl:DatatypeProperty .
:hasCause rdfs:domain :WildfireIncident .
:hasCause rdfs:range xsd:string .

:hasAreaAffected rdf:type owl:DatatypeProperty .
:hasAreaAffected rdfs:domain :WildfireIncident .
:hasAreaAffected rdfs:range xsd:float .

:hasWeatherCondition rdf:type owl:ObjectProperty .
:hasWeatherCondition rdfs:domain :WildfireIncident .
:hasWeatherCondition rdfs:range :WeatherCondition .

:Forest rdf:type owl:Class .
:Forest rdfs:subClassOf :AreaType .

:Grassland rdf:type owl:Class .
:Grassland rdfs:subClassOf :AreaType .

:AgricultureLand rdf:type owl:Class .
:AgricultureLand rdfs:subClassOf :AreaType .

:longitude rdf:type owl:DatatypeProperty .
:longitude rdfs:domain :Location .
:longitude rdfs:range xsd:float .

:latitude rdf:type owl:DatatypeProperty .
:latitude rdfs:domain :Location .
:latitude rdfs:range xsd:float .

:altitude rdf:type owl:DatatypeProperty .
:altitude rdfs:domain :Location .
:altitude rdfs:range xsd:float .

:hasService rdf:type owl:ObjectProperty .
:hasService rdfs:domain :Mission .
:hasService rdfs:range :Service .

:hasPopulationDensity rdf:type owl:DatatypeProperty .
:hasPopulationDensity rdfs:domain :Mission .
:hasPopulationDensity rdfs:range xsd:float .

:hasPeopleInvolved rdf:type owl:ObjectProperty .
:hasPeopleInvolved rdfs:domain :Mission .
:hasPeopleInvolved rdfs:range :Person .

:StatusOfForestryWork rdf:type owl:ObjectProperty .
:StatusOfForestryWork rdfs:domain :WildfireIncident .
:StatusOfForestryWork rdfs:range :Firebreak, :AccessToForestRoads .

:areaInAcres rdf:type owl:DatatypeProperty .
:areaInAcres rdfs:domain :WildfireIncident .
:areaInAcres rdfs:range xsd:float .