@prefix : <http://example.org/sar#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Classes
:Incident rdf:type owl:Class .
:Impact rdf:type owl:Class .
:WeatherCondition rdf:type owl:Class .
:SensorData rdf:type owl:Class .
:SocialMediaData rdf:type owl:Class .
:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .

# SubClasses
:Wildfire rdf:type owl:Class ; rdfs:subClassOf :Incident .
:Smoke rdf:type owl:Class ; rdfs:subClassOf :Incident .
:Damage rdf:type owl:Class ; rdfs:subClassOf :Impact .

:Temperature rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:WindSpeed rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:Humidity rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .

# Properties
:hasWeatherCondition rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :WeatherCondition .
:hasImpact rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Impact .
:isMonitoredBy rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :SensorData .
:isReportedBy rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :SocialMediaData .
:hasMission rdf:type owl:ObjectProperty ; rdfs:domain :Service ; rdfs:range :Mission .
:hasService rdf:type owl:ObjectProperty ; rdfs:domain :Mission ; rdfs:range :Service .
:hasLocation rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range xsd:string .
:hasPriority rdf:type owl:DatatypeProperty ; rdfs:domain :Incident ; rdfs:range xsd:string .
:hasUrgency rdf:type owl:DatatypeProperty ; rdfs:domain :Incident ; rdfs:range xsd:string .

# Competency Questions Representation
# CQ1: What are the most important weather variables that can cause forest fire?
:Wildfire rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasWeatherCondition ;
  owl:someValuesFrom :WeatherCondition
] .

# CQ2: What are the current measurements for these weather variables?
:WeatherCondition rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :isMonitoredBy ;
  owl:someValuesFrom :SensorData
] .

# CQ3: What is the forecast for the weather in this location?
:WeatherForecast rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:hasForecast rdf:type owl:ObjectProperty ; rdfs:domain :Location ; rdfs:range :WeatherForecast .

# CQ4: Where did the incident take place?
:Incident rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasLocation ;
  owl:someValuesFrom xsd:string
] .

# CQ5: What is the priority of an incident during a forest fire disaster?
:Incident rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasPriority ;
  owl:someValuesFrom xsd:string
] .

# CQ6: What incidents during forest fires are the most urgent?
:Incident rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasUrgency ;
  owl:someValuesFrom xsd:string
] .

# CQ7: What data from the source are depicted?
:SensorData rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :isMonitoredBy ;
  owl:someValuesFrom :Incident
] .

# CQ8: Which is the creation date of these data?
:hasCreationDate rdf:type owl:DatatypeProperty ; rdfs:domain :SensorData ; rdfs:range xsd:dateTime .

# CQ9: What is the location of this item?
:Item rdf:type owl:Class .
:hasItemLocation rdf:type owl:ObjectProperty ; rdfs:domain :Item ; rdfs:range xsd:string .

# CQ10: Which is the classification type of smoke?
:Smoke rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasClassification ;
  owl:someValuesFrom xsd:string
] .

# CQ11: Which vulnerable objects were involved in the incident?
:VulnerableObject rdf:type owl:Class .
:hasVulnerableObject rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :VulnerableObject .

# CQ12: What is the status of wildfire forestry works (firebreaks, access to forest roads, etc.)?
:ForestryWork rdf:type owl:Class .
:hasForestryWorkStatus rdf:type owl:DatatypeProperty ; rdfs:domain :ForestryWork ; rdfs:range xsd:string .

# CQ13: What services or support do you offer for firefighting?
:FirefightingSupport rdf:type owl:Class ; rdfs:subClassOf :Service .

# CQ14: Which mission do you follow for this support/service?
:hasMission rdf:type owl:ObjectProperty ; rdfs:domain :FirefightingSupport ; rdfs:range :Mission .

# CQ15: What is the location where this mission is taking place?
:Mission rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasLocation ;
  owl:someValuesFrom xsd:string
] .

# CQ16: Where is the most urgent mission taking place?
:Mission rdfs:subClassOf [
  rdf:type owl:Restriction ;
  owl:onProperty :hasUrgency ;
  owl:someValuesFrom xsd:string
] .

# CQ17: What is the population density in the area?
:Location rdf:type owl:Class .
:hasPopulationDensity rdf:type owl:DatatypeProperty ; rdfs:domain :Location ; rdfs:range xsd:integer .

# CQ18: What is the location of the involved people?
:Person rdf:type owl:Class .
:hasPersonLocation rdf:type owl:ObjectProperty ; rdfs:domain :Person ; rdfs:range :Location .