@prefix : <http://example.com/sar-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Incident rdf:type owl:Class .
:WeatherCondition rdf:type owl:Class .
:SensorData rdf:type owl:Class .
:SocialMediaData rdf:type owl:Class .
:FirstResponderInput rdf:type owl:Class .
:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .
:VulnerableObject rdf:type owl:Class .
:Impact rdf:type owl:Class .
:Location rdf:type owl:Class .
:Priority rdf:type owl:Class .

:Temperature rdf:type owl:Class .
:WindSpeed rdf:type owl:Class .
:Humidity rdf:type owl:Class .
:WeatherForecast rdf:type owl:Class .

:hasIncidentLocation rdf:type owl:ObjectProperty .
:hasIncidentLocation rdfs:domain :Incident .
:hasIncidentLocation rdfs:range :Location .

:hasPriority rdf:type owl:ObjectProperty .
:hasPriority rdfs:domain :Incident .
:hasPriority rdfs:range :Priority .

:hasImpact rdf:type owl:ObjectProperty .
:hasImpact rdfs:domain :Incident .
:hasImpact rdfs:range :Impact .

:hasWeatherCondition rdf:type owl:ObjectProperty .
:hasWeatherCondition rdfs:domain :Incident .
:hasWeatherCondition rdfs:range :WeatherCondition .

:hasSensorData rdf:type owl:ObjectProperty .
:hasSensorData rdfs:domain :Incident .
:hasSensorData rdfs:range :SensorData .

:hasSocialMediaData rdf:type owl:ObjectProperty .
:hasSocialMediaData rdfs:domain :Incident .
:hasSocialMediaData rdfs:range :SocialMediaData .

:hasFirstResponderInput rdf:type owl:ObjectProperty .
:hasFirstResponderInput rdfs:domain :Incident .
:hasFirstResponderInput rdfs:range :FirstResponderInput .

:hasMission rdf:type owl:ObjectProperty .
:hasMission rdfs:domain :Service .
:hasMission rdfs:range :Mission .

:hasService rdf:type owl:ObjectProperty .
:hasService rdfs:domain :Mission .
:hasService rdfs:range :Service .

:hasWeatherVariable rdf:type owl:ObjectProperty .
:hasWeatherVariable rdfs:domain :WeatherCondition .
:hasWeatherVariable rdfs:range :WeatherVariable .

:WeatherVariable rdf:type owl:Class .
:WeatherVariable owl:unionOf (:Temperature :WindSpeed :Humidity :WeatherForecast) .

:hasCreationDate rdf:type owl:DatatypeProperty .
:hasCreationDate rdfs:domain :SensorData .
:hasCreationDate rdfs:range xsd:dateTime .

:hasLocation rdf:type owl:ObjectProperty .
:hasLocation rdfs:domain :SensorData .
:hasLocation rdfs:range :Location .

:hasSmokeClassification rdf:type owl:DatatypeProperty .
:hasSmokeClassification rdfs:domain :SensorData .
:hasSmokeClassification rdfs:range xsd:string .

:hasVulnerableObject rdf:type owl:ObjectProperty .
:hasVulnerableObject rdfs:domain :Incident .
:hasVulnerableObject rdfs:range :VulnerableObject .

:hasStatus rdf:type owl:DatatypeProperty .
:hasStatus rdfs:domain :Incident .
:hasStatus rdfs:range xsd:string .

:hasPopulationDensity rdf:type owl:DatatypeProperty .
:hasPopulationDensity rdfs:domain :Location .
:hasPopulationDensity rdfs:range xsd:integer .

:hasInvolvedPeople rdf:type owl:ObjectProperty .
:hasInvolvedPeople rdfs:domain :Incident .
:hasInvolvedPeople rdfs:range :Person .

:Person rdf:type owl:Class .