@prefix : <http://example.com/ontology/sar#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/ontology/sar> owl:imports <http://example.org/otherOntologies/weather.owl> .

:Incident rdf:type owl:Class .
:Wildfire rdf:type owl:Class ; rdfs:subClassOf :Incident .
:Resurgence rdf:type owl:Class ; rdfs:subClassOf :Incident .
:FalseAdvertisement rdf:type owl:Class ; rdfs:subClassOf :Incident .

:Impact rdf:type owl:Class .
:EnvironmentalImpact rdf:type owl:Class ; rdfs:subClassOf :Impact .
:HumanImpact rdf:type owl:Class ; rdfs:subClassOf :Impact .
:EconomicImpact rdf:type owl:Class ; rdfs:subClassOf :Impact .

:WeatherCondition rdf:type owl:Class .
:Temperature rdf:type owl:DatatypeProperty .
:WindSpeed rdf:type owl:DatatypeProperty .
:Humidity rdf:type owl:DatatypeProperty .
:WeatherForecast rdf:type owl:DatatypeProperty .

:SensorData rdf:type owl:Class .
:SatelliteData rdf:type owl:Class ; rdfs:subClassOf :SensorData .
:SocialMediaData rdf:type owl:Class ; rdfs:subClassOf :SensorData .
:FirstResponderInput rdf:type owl:Class ; rdfs:subClassOf :SensorData .

:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .

:hasImpact rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Impact .
:hasLocation rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Location .
:hasTime rdf:type owl:DatatypeProperty ; rdfs:domain :Incident ; rdfs:range xsd:dateTime .
:hasWeatherCondition rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :WeatherCondition .
:hasSensorData rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :SensorData .
:hasSatelliteData rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :SatelliteData .
:hasSocialMediaData rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :SocialMediaData .
:hasFirstResponderInput rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :FirstResponderInput .
:hasMission rdf:type owl:ObjectProperty ; rdfs:domain :Service ; rdfs:range :Mission .
:hasPriority rdf:type owl:DatatypeProperty ; rdfs:domain :Incident ; rdfs:range xsd:string .
:hasUrgency rdf:type owl:DatatypeProperty ; rdfs:domain :Incident ; rdfs:range xsd:string .
:hasStatus rdf:type owl:DatatypeProperty ; rdfs:domain :ForestryWork ; rdfs:range xsd:string .
:hasPopulationDensity rdf:type owl:DatatypeProperty ; rdfs:domain :Location ; rdfs:range xsd:integer .

:Location rdf:type owl:Class .
:ForestryWork rdf:type owl:Class .

:FireBreak rdf:type owl:Class ; rdfs:subClassOf :ForestryWork .
:ForestRoad rdf:type owl:Class ; rdfs:subClassOf :ForestryWork .

:hasFireBreak rdf:type owl:ObjectProperty ; rdfs:domain :Location ; rdfs:range :FireBreak .
:hasForestRoad rdf:type owl:ObjectProperty ; rdfs:domain :Location ; rdfs:range :ForestRoad .

:hasService rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Service .
:hasInvolvedPeople rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :Person .
:hasVulnerableObject rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :VulnerableObject .

:Person rdf:type owl:Class .
:VulnerableObject rdf:type owl:Class .

:FireVehicle rdf:type owl:Class ; rdfs:subClassOf :Vehicle .
:Vehicle rdf:type owl:Class .

:hasFireVehicle rdf:type owl:ObjectProperty ; rdfs:domain :Incident ; rdfs:range :FireVehicle .
:hasDistanceFromFireVehicle rdf:type owl:DatatypeProperty ; rdfs:domain :Incident ; rdfs:range xsd:float .

:hasSmokeClassification rdf:type owl:DatatypeProperty ; rdfs:domain :Incident ; rdfs:range xsd:string .

:hasCreationDate rdf:type owl:DatatypeProperty ; rdfs:domain :SensorData ; rdfs:range xsd:dateTime .
:hasItemLocation rdf:type owl:ObjectProperty ; rdfs:domain :SensorData ; rdfs:range :Location .

:WeatherCondition owl:equivalentClass [
  rdf:type owl:Class ;
  owl:unionOf (:Temperature :WindSpeed :Humidity :WeatherForecast)
] .

:Incident owl:equivalentClass [
  rdf:type owl:Class ;
  owl:unionOf (:Wildfire :Resurgence :FalseAdvertisement)
] .

:Impact owl:equivalentClass [
  rdf:type owl:Class ;
  owl:unionOf (:EnvironmentalImpact :HumanImpact :EconomicImpact)
] .

:ForestryWork owl:equivalentClass [
  rdf:type owl:Class ;
  owl:unionOf (:FireBreak :ForestRoad)
] .