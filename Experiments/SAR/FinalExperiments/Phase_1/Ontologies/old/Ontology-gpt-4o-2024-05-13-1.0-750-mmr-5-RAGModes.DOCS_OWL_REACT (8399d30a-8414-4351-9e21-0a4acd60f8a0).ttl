@prefix : <http://example.com/owl/sar/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:WildfireIncident rdf:type owl:Class .
:Impact rdf:type owl:Class .
:WeatherCondition rdf:type owl:Class .
:ObservationData rdf:type owl:Class .
:Mission rdf:type owl:Class .
:Service rdf:type owl:Class .

:hasLocation rdf:type owl:ObjectProperty ; rdfs:domain :WildfireIncident .
:hasPriority rdf:type owl:DatatypeProperty ; rdfs:domain :WildfireIncident .
:isUrgent rdf:type owl:DatatypeProperty ; rdfs:domain :WildfireIncident .
:hasIncidentType rdf:type owl:ObjectProperty ; rdfs:domain :WildfireIncident .
:hasFireType rdf:type owl:ObjectProperty ; rdfs:domain :WildfireIncident .
:hasFireBreakStatus rdf:type owl:ObjectProperty ; rdfs:domain :WildfireIncident .
:hasPopulationDensity rdf:type owl:DatatypeProperty ; rdfs:domain :WildfireIncident .
:isVulnerable rdf:type owl:ObjectProperty ; rdfs:domain :WildfireIncident ; rdfs:range :VulnerableObject .

:Temperature rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:WindSpeed rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .
:Humidity rdf:type owl:Class ; rdfs:subClassOf :WeatherCondition .

:hasTemperature rdf:type owl:DatatypeProperty .
:hasWindSpeed rdf:type owl:DatatypeProperty .
:hasHumidity rdf:type owl:DatatypeProperty .
:hasForecast rdf:type owl:DatatypeProperty ; rdfs:domain :WeatherCondition .

:ObservationData rdf:type owl:Class .
:depicts rdf:type owl:ObjectProperty ; rdfs:domain :ObservationData .
:creationDate rdf:type owl:DatatypeProperty ; rdfs:domain :ObservationData .
:observationLocation rdf:type owl:ObjectProperty ; rdfs:domain :ObservationData .

:Mission rdf:type owl:Class .
:involvesService rdf:type owl:ObjectProperty ; rdfs:domain :Mission .
:missionLocation rdf:type owl:ObjectProperty ; rdfs:domain :Mission .

:Service rdf:type owl:Class .
:hasService rdf:type owl:ObjectProperty ; rdfs:domain :Service .
:followsService rdf:type owl:ObjectProperty ; rdfs:domain :Service .

:IncidentType rdf:type owl:Class .
:FireType rdf:type owl:Class .
:FireBreakStatus rdf:type owl:Class .
:VulnerableObject rdf:type owl:Class ;
  rdfs:subClassOf :Impact .

:GarbageLandfill rdf:type :IncidentType .
:MethodNotification rdf:type owl:Class .