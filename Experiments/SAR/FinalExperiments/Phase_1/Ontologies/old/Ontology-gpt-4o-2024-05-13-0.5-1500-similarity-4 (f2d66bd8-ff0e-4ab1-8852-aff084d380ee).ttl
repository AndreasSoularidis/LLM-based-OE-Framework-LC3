@prefix : <http://example.com/owl/sar/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/owl/sar> rdf:type owl:Ontology .

:Incident rdf:type owl:Class ;
    rdfs:comment "Represents an incident during a wildfire." .

:WeatherCondition rdf:type owl:Class ;
    rdfs:comment "Represents weather conditions relevant to wildfire incidents." .

:DataSource rdf:type owl:Class ;
    rdfs:comment "Represents various data sources like sensors, satellites, and social media." .

:Mission rdf:type owl:Class ;
    rdfs:comment "Represents missions involved in wildfire management." .

:Service rdf:type owl:Class ;
    rdfs:comment "Represents services involved in wildfire management." .

:location rdf:type owl:ObjectProperty ;
    rdfs:domain :Incident, :Mission, :DataSource ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the location of an incident, mission, or data source." .

:priority rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the priority of an incident." .

:incidentType rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the type of incident." .

:formOfFire rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the form of fire in an incident." .

:startingPoint rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the starting point of a fire." .

:startingPosition rdf:type owl:DatatypeProperty ;
    rdfs:domain :Incident ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the starting position of a fire." .

:temperature rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float ;
    rdfs:comment "Represents the temperature in weather conditions." .

:windSpeed rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float ;
    rdfs:comment "Represents the wind speed in weather conditions." .

:humidity rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:float ;
    rdfs:comment "Represents the humidity in weather conditions." .

:forecast rdf:type owl:DatatypeProperty ;
    rdfs:domain :WeatherCondition ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the weather forecast." .

:creationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :DataSource ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "Represents the creation date of data from a data source." .

:dataType rdf:type owl:DatatypeProperty ;
    rdfs:domain :DataSource ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the type of data from a data source." .

:urgency rdf:type owl:DatatypeProperty ;
    rdfs:domain :Mission ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the urgency of a mission." .

:populationDensity rdf:type owl:DatatypeProperty ;
    rdfs:domain :Mission ;
    rdfs:range xsd:float ;
    rdfs:comment "Represents the population density in the area of a mission." .

:involvedPeopleLocation rdf:type owl:ObjectProperty ;
    rdfs:domain :Mission ;
    rdfs:range xsd:string ;
    rdfs:comment "Represents the location of people involved in a mission." .

:providesService rdf:type owl:ObjectProperty ;
    rdfs:domain :Mission ;
    rdfs:range :Service ;
    rdfs:comment "Represents the services provided during a mission." .

:hasMission rdf:type owl:ObjectProperty ;
    rdfs:domain :Service ;
    rdfs:range :Mission ;
    rdfs:comment "Represents the mission a service is involved in." .

:hasWeatherCondition rdf:type owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range :WeatherCondition ;
    rdfs:comment "Represents the weather conditions during an incident." .

:hasDataSource rdf:type owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range :DataSource ;
    rdfs:comment "Represents the data sources relevant to an incident." .

:hasIncident rdf:type owl:ObjectProperty ;
    rdfs:domain :Mission ;
    rdfs:range :Incident ;
    rdfs:comment "Represents the incidents involved in a mission." .

:IncidentImpact rdf:type owl:Class ;
    rdfs:comment "Represents the impact of an incident." .

:hasImpact rdf:type owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range :IncidentImpact ;
    rdfs:comment "Represents the impact of an incident." .

:VulnerableObject rdf:type owl:Class ;
    rdfs:comment "Represents vulnerable objects involved in an incident." .

:involvesVulnerableObject rdf:type owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range :VulnerableObject ;
    rdfs:comment "Represents the vulnerable objects involved in an incident." .

:SmokeClassification rdf:type owl:Class ;
    rdfs:comment "Represents the classification type of smoke." .

:hasSmokeClassification rdf:type owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range :SmokeClassification ;
    rdfs:comment "Represents the classification type of smoke in an incident." .

:ForestryWorkStatus rdf:type owl:Class ;
    rdfs:comment "Represents the status of forestry works like firebreaks and access to forest roads." .

:hasForestryWorkStatus rdf:type owl:ObjectProperty ;
    rdfs:domain :Incident ;
    rdfs:range :ForestryWorkStatus ;
    rdfs:comment "Represents the status of forestry works during an incident." .